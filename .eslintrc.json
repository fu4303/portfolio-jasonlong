{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "semi": 0,
      "react/jsx-filename-extension": 0,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "comma-dangle": 0
    }
}
